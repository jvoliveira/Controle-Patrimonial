        <Label alignment="CENTER" layoutX="155.0" layoutY="177.0" style="&#10;" text="Drag components from Library here…" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <TabPane layoutY="-2.0" prefHeight="408.0" prefWidth="640.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                  <Tab closable="false" text="Manutenções">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="379.0" prefWidth="628.0">
                           <children>
                              <TableView fx:id="tbManut" layoutX="14.0" layoutY="60.0" prefHeight="305.0" prefWidth="613.0">
                                <columns>
                                  <TableColumn prefWidth="75.0" text="C1" />
                                  <TableColumn prefWidth="75.0" text="C2" />
                                </columns>
                              </TableView>
                              <Button fx:id="btATUALIZAmanut" layoutX="563.0" layoutY="27.0" mnemonicParsing="false" onMouseClicked="#AtualizarTBManutencao" text="Atualizar" />
                              <Button fx:id="btNOVOmanut" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onMouseClicked="#novaManutencao" text="Novo" />
