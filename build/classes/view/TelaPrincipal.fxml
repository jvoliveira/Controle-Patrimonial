                              <TextField fx:id="tfPesquisaPat" layoutX="402.0" layoutY="27.0" onKeyTyped="#PesquisaPatrimonios" promptText="Pesquisar..." />
                           </children>
                        </AnchorPane>
                     </content>
                  </Tab>
                  <Tab closable="false" text="Equipamentos">
