                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                              <Button fx:id="btATUALIZAequip" layoutX="563.0" layoutY="27.0" mnemonicParsing="false" onMouseClicked="#AtualizaTBEquipamento" text="Atualizar" />
