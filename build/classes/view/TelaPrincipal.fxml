                              <Button fx:id="btEXCLUIequip" layoutX="122.0" layoutY="15.0" mnemonicParsing="false" onMouseClicked="#ExcluirEquipamento" text="Excluir" />
                              <TextField fx:id="tfPesquisaEquip" layoutX="402.0" layoutY="27.0" onKeyTyped="#PesquisaEquipamentos" promptText="Pesquisar..." />
                           </children>
                        </AnchorPane>
                     </content>
