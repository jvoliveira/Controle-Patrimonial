                        </AnchorPane>
                    </content>
                  </Tab>
                  <Tab closable="false" text="Patrimônios">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="379.0" prefWidth="628.0">
                           <children>
                              <TableView fx:id="tbPat" layoutX="14.0" layoutY="60.0" prefHeight="305.0" prefWidth="613.0">
                                 <columns>
                                    <TableColumn prefWidth="75.0" text="C1" />
                                    <TableColumn prefWidth="75.0" text="C2" />
                                 </columns>
                              </TableView>
                              <Button fx:id="btATUALIZApat" layoutX="563.0" layoutY="27.0" mnemonicParsing="false" onMouseClicked="#AtualizaTBPatrimonio" text="Atualizar" />
                              <Button fx:id="btNOVOpat" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onMouseClicked="#novoPatrimonio" text="Novo" />
